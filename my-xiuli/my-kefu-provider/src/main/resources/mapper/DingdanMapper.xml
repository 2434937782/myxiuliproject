<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.kgc.mapper.DingdanMapper">
    <select id="getAllDindan" resultType="com.kgc.pojo.Dingdan">
        SELECT * FROM dingdan
    </select>

    <select id="getByphone" resultType="com.kgc.pojo.Dingdan">
        SELECT * FROM dingdan
        where djiphone=#{djiphone}
    </select>

</mapper>  